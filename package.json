    "@loopback/eslint-config": "^6.0.3",
    "@loopback/testlab": "^3.0.1",
    "@types/async": "^3.2.8",
    "@types/axios": "^0.14.0",
    "@types/capitalize": "^2.0.0",
    "@types/chalk": "^2.2.0",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.172",
    "@types/loopback": "^3.2.10",
