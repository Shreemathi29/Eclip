{
  "name": "@vlinder-be/trag-vlinder-core-application-node",
  "alias": "trag-vlinder-core-application-node",
  "version": "0.25.0",
  "description": "trag-vlinder-core-application-node",
  "keywords": [
    "trag-vlinder-core-application-node",
    "vlinder"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "!*/__tests__",
    "Secretfile.*",
    "index.js",
    "compiled.js",
    "src/config/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc && tsc-alias",
    "build:watch": "lb-tsc --watch",
    "postbuild": "cpx \"src/**/*.{proto, jpg, jpeg, svg, css, html}\" dist ",
    "uglify": "uglifyjs-folder dist -e -x .js -o dist",
    "prebuild": "npm run clean && npm run prettier:fix",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.{js,ts,tsx,json}\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "NODE_ENV=test npm run build",
    "pretestReport": "NODE_ENV=test npm run build",
    "test": "npx majestic && majestic",
    "testReport": "jest",
    "docker:build": "docker build -t trag-vlinder-core-application-node .",
    "docker:run": "docker run -p 3000:3000 -d trag-vlinder-core-application-node",
    "migrate:alter": "node ./dist/migrate",
    "migrate:drop": "node ./dist/migrate --rebuild",
    "prestart": "npm run build",
    "postinstall": "patch-package && husky install",
    "start": "node  --enable-source-maps ./index.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "commit": "npx git-cz",
    "release": "release-it",
    "gen-proto-ts": "pbjs -t static-module -w commonjs -o compiled.js  ./src/servers/grpc/protos/ks1.proto",
    "postgen-proto-ts": "pbts -o compiled.d.ts compiled.js",
    "tarball": "npm run build && npx npm-pack-all",
    "production_start": "node  --enable-source-maps ./index.js"
  },
  "repository": "git@github.com:vlinderlabs/trag-vlinder-core-application-node.git",
  "homepage": "https://github.com/vlinderlabs/trag-vlinder-core-application-node#readme",
  "license": "Vlinder Labs Pvt. Ltd",
  "author": {
    "name": "vlinder Labs Pvt. Ltd.",
    "email": "developer@vlinder.io",
    "url": "https://vlinder.io"
  },
